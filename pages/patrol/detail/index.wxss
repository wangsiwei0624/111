@import '../../../static/icon/iconfont.wxss';
page{
  height: 100%;
}
.page{
  height: 100%;
  width: 100%;
  border-top: 1rpx solid #E4EBEE; 
  background: #f7f9fa;
}
.wux-whitespace--default{
  background: #f7f9fa;
} 
/* .wux-whitespace:nth-child()--default{
  background: #ffffff;
} */
.wuxform{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.flex{
  /* flex: 1; */
  overflow: auto;
  /* background: #f7f9fa; */
}
.wux-cell-group{
  background: #ffffff;
  border-bottom: 1rpx solid #EEEEEE;
}

.wux-cell{
  width: 100%;
  display: flex;
  margin: 0rpx auto !important ;
  padding: 20rpx 0rpx !important; 
  background: #ffffff !important;
  border-bottom: 1rpx solid #EEEEEE;
}
.wux-cell-feedback{
  width: 90%;
  margin: 0 auto;
  /* margin-left: 30rpx;  */
}
.wuxcell{
  background: #ffffff !important; 
}
.wux-cell--hover::active{
  background: #ffffff !important;
}
.wux-cell::after{
  border-bottom: 0rpx solid #d9d9d9 !important;
}
.wuxcellgroup-remarks{

  width: 100%;
}
.message{
  height: 80rpx;
  width: 100%;
  background: #ffffff;
  line-height: 60rpx;
  border-bottom: 1rpx solid #EEEEEE; 
}
.oblong{
  height: 30rpx;
  width: 7rpx;
  margin-left: 30rpx;
  margin-top: 25rpx;
  background: #2A7AD7;
  line-height: 80rpx;
  float: left;
}
.message text{
  line-height: 80rpx;
  font-size: 30rpx;
  color: #2A7AD7;
  font-weight: bold;
  margin-left: 5rpx;
  float: left;
  /* padding-left: 5rpx; */
}
.text-name text{
  line-height: 80rpx;
  font-size: 28rpx;
  color: #7D9AB6;
  font-weight: 400;
}
.wuxcellgroup .wux-cell{
  border-bottom:1rpx solid #EEEEEE;
  width: 90%;
}
.wuxcellgroup .wux-cell--last{
  border-bottom:0rpx solid #EEEEEE;
}
.wux-cell text{
  line-height: 60rpx;
}
.textleft{
  /* margin-left: 20rpx; */
  width: 21%;
  font-size: 28rpx;
  color: #333333;
  font-weight:400;
  float: left;
}
.textright{
  width: 78%;
  font-size: 28rpx;
  color: #7D9AB6;
  font-weight:400;
  float: left;
  /* margin-right: 30rpx; */
}
.textleftlocation{
  /* margin-left: 20rpx; */
  width: 22%;
  font-size: 28rpx;
  color: #333333;
  font-weight:400;
  float: left;
}
.textrightlocation{
  width: 67%;
  font-size: 28rpx;
  color: #7D9AB6;
  font-weight:400;
  float: left;
}
.text-remarks{
  font-size: 28rpx;
  color: #7D9AB6;
  font-weight:400; 
}
.imgandvideo {
  display: flex;
  flex-wrap: wrap;
}
.imgandvideo .show-attach .info-img video{
  height: 150rpx !important;
  width: 220rpx !important;
  margin-right: 20rpx; 
}
.iconfont{
  color: #1EBD0B;
  float: right;
  font-size: 44rpx;
  /* margin-right: 30rpx;  */
}
.wuxbutton{
  width: 100%;
  position: fixed;
  bottom: 0;
} 
.wux-button--block{
  margin-top: 0rpx !important;
  margin-bottom: 0rpx !important;
}
.wux-button--full{
  margin-top: 0rpx !important;
  margin-bottom: 0rpx !important;
}
wux-button.wuxbutton button {
  border-radius: 0px;
}