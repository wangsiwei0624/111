@import '../../static/icon/iconfont.wxss';

page{
  height: 100%;
}
.page{
  position: fixed;
  left: 0px;
  bottom: 0px;
  top:0px;
  right: 0px;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  background: #F7F9FA;
}
.page__hd{
  flex: 1;
  overflow: auto;
}
.hd-banner-img{
  height: 300rpx;
  width: 100%;
  z-index: 1;
}
.hd-layout{
  z-index: 1;
}
.hd-layout-col1 .placeholder{
  height: 150rpx;
  width: 340rpx;
  background: #2ABDAE;
  margin-top: 20rpx;
  margin-left: 30rpx;
  border-radius: 10rpx;
  position: relative;
}
.hd-layout-col2 .placeholder{
  height: 150rpx;
  width: 340rpx;
  background: #C63D51;
  margin-top: 20rpx;
  margin-left: 10rpx;
  border-radius: 10rpx;
  position: relative;
}
.hd-layout-col3 .placeholder{
  height: 150rpx;
  width: 340rpx;
  background: #2762D2;
  margin-top: 10rpx;
  margin-left: 30rpx;
  border-radius: 10rpx;
  position: relative;
}
.hd-layout-col4 .placeholder{
  height: 150rpx;
  width: 340rpx;
  background: #D9881A;
  margin-top: 10rpx;
  margin-left: 10rpx;
  border-radius: 10rpx;
  position: relative;
}
.hd-layout .placeholder text{
  font-size: 28rpx;
  color: #ffffff; 
  position: absolute;
  top: 65rpx;
  left: 153rpx;
}
.hd-layout .placeholder .iconfont{
  color: #ffffff;
  font-size: 56rpx;
  position: absolute;
  top: 48rpx;
  left: 76rpx;
}

.hd-layout-else{
  height: 150rpx;
  width: 100%;
  background: #ffffff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1;
}
.hd-layout-col1-else .placeholder{
  height: 110rpx;
  width: 160rpx;
  background: #2ABDAE;
  margin-left: 30rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
}
.hd-layout-col2-else .placeholder{
  height: 110rpx;
  width: 160rpx;
  background: #C63D51;
  margin-left: 18rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
}
.hd-layout-col3-else .placeholder{
  height: 110rpx;
  width: 160rpx;
  background: #2762D2;
  margin-left: 12rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
}
.hd-layout-col4-else .placeholder{
  height: 110rpx;
  width: 160rpx;
  background: #D9881A;
  margin-left: 5rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
}
.hd-layout-else .placeholder .iconfont{
  font-size: 40rpx;
  color: #ffffff;
  margin: 0rpx auto;
  margin-top: 20rpx;
}
.hd-layout-else .placeholder text{
  font-size: 24rpx;
  color: #ffffff;
  margin: 0rpx auto;
  margin-top: 10rpx;
}


/* list样式 */
.list{
  width: 100%;
}
.list-in{
  width: 690rpx;
  margin: 35rpx auto;
}
.list-in-cell-title{
  width: 690rpx;
  height: 48rpx;
  margin-top: 30rpx; 
}
.title1{
  width: 80rpx;
  height: 30rpx;
  float: left;
  border-bottom: 18rpx solid #9BC0EC;
}
.title1 .text{
  display: flex;
  line-height: 45rpx;
  font-size:34rpx;
  /* font-family:PingFangSC; */
  font-weight:600;
  color: #333333;
  margin-left: 6rpx;
  
}
.title2{
  width: 88rpx;
  height: 48rpx;
  background:linear-gradient(154deg,rgba(255,213,53,1) 0%,rgba(246,133,12,1) 100%);
  border-radius: 23rpx;
  float: right;
}
.title2 .text{
  display: flex;
  font-size: 22rpx;
  color: #ffffff;
  margin-left: 22rpx;
  line-height: 48rpx;
}
.list-in-cell-no{
  height: 100rpx;
  width: 690rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  background: #ffffff;
}
.list-in-cell-no text{
  display: flex;
  justify-content:center; 
  font-size: 28rpx;
  color: #BFCEDD;
  line-height: 100rpx;
  
}
.list-in-cell-detail{
  width: 690rpx;
  margin: 33rpx auto;
}
.list-in-cell-detail-cell{
  width: 690rpx;
  /* height: 134rpx; */
  background:#ffffff;
  border-radius:5px;
  margin: 10rpx auto;
}
.list-in-cell-detail-cell .detail-theme{
  width: 650rpx;
  margin: 20rpx auto;
  font-size: 30rpx;
  line-height: 50rpx;
  margin-top: 21rpx;
  margin-left: 20rpx;
  font-weight:400;
  text-overflow:ellipsis;
      white-space: nowrap;
  color: #333333;
}
.list-in-cell-detail-cell .detail-img{
  /* margin-top: 14rpx; */
  margin-left: 20rpx; 
}
.list-in-cell-detail-cell .detail-img .iconfont{
  font-size: 30rpx;
  margin-right: 10rpx;
  color: #A8BCD0;
}
.list-in-cell-detail-cell .detail-address{
  font-size: 24rpx;
  color: #A8BCD0;
  /* margin-top: 18rpx; */
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  line-height: 66rpx;
}
.list-in-cell-detail-cell .detail-address .iconfont{
  font-size: 24rpx;
  margin-right: 5rpx;
}
.list-in-cell-detail-cell .detail-address .time{
  float: right;
  margin-right: 18rpx;
}
.list-in-cell-detail-cell .detail-timeslot{
  font-size: 24rpx;
  color: #A8BCD0;
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.list-in-cell-detail-cell .detail-timeslot .iconfont{
  font-size:  30rpx;
  margin-right: 5rpx;
}

.page__bd{
  box-shadow:0px -2rpx 5rpx 0rpx rgba(0,0,0,0.1);
}

