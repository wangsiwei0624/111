@import '../../../static/icon/iconfont.wxss';
page{
  height: 100%;
}
.page{
  height: 100%;
  width: 100%;
  border-top: 1rpx solid #E4EBEE; 
  background: #f7f9fa;
}
.wux-whitespace--default{
  background: #f7f9fa;
} 
/* .wux-whitespace:nth-child()--default{
  background: #ffffff;
} */
.wuxform{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.flex{
  /* flex: 1; */
  overflow: auto;
  /* background: #f7f9fa; */
}
.wux-cell-group{
  background: #ffffff;
}
.wux-cell:nth-child(2){
  width: 100%;
  padding: 20rpx 30rpx !important;
}
.wux-cell{
  width: 94%;
  display: flex;
  margin: 1rpx auto !important ;
  padding: 20rpx 10rpx !important; 
  background: #ffffff !important; 
}
.wuxcell{
  background: #ffffff !important; 
}
.wux-cell--hover::active{
  background: #ffffff !important;
}
.wux-cell::after{
  border-bottom: 0rpx solid #d9d9d9 !important;
}
.message{
  height: 80rpx;
  width: 100%;
  background: #ffffff;
  line-height: 60rpx;
  border-bottom: 1rpx solid #EEEEEE; 
}
.oblong{
  height: 30rpx;
  width: 7rpx;
  margin-left: 30rpx;
  margin-top: 25rpx;
  background: #2A7AD7;
  line-height: 80rpx;
  float: left;
}
.message text{
  line-height: 80rpx;
  font-size: 30rpx;
  color: #2A7AD7;
  font-weight: bold;
  margin-left: 5rpx;
  float: left;
  /* padding-left: 5rpx; */
}
.text-name text{
  line-height: 80rpx;
  font-size: 28rpx;
  color: #7D9AB6;
}
.wux-cell{
  border-bottom:1rpx solid #EEEEEE;
}
.wux-cell:nth-last-child(2){
  border-bottom: 0rpx
}
.wux-cell text{
  line-height: 60rpx;
}
.textleft{
  width: 22%;
  font-size: 28rpx;
  color: #333333;
  font-weight:400;
  float: left;
}
.textright{
  width: 78%;
  font-size: 28rpx;
  color: #7D9AB6;
  font-weight:400;
  float: left;
}
.textleftlocation{
  width: 22%;
  font-size: 28rpx;
  color: #333333;
  font-weight:400;
  float: left;
}
.textrightlocation{
  width: 70%;
  font-size: 28rpx;
  color: #7D9AB6;
  font-weight:400;
  float: left;
}
.text-remarks{
  font-size: 28rpx;
  color: #7D9AB6;
  font-weight:400; 
}
.imgandvideo {
  display: flex;
  flex-wrap: wrap;
}
.own image{
  width: 220rpx;
  height: 150rpx;
  margin-top: 10rpx;
  border: 1rpx solid #EEEEEE;
  margin-right: 5rpx;
}
.own video{
  width: 220rpx;
  height: 150rpx;
  margin-top: 10rpx;
  border: 1rpx solid #EEEEEE;
  margin-right: 5rpx;
}
.iconfont{
  color: #1EBD0B;
  float: right;
  font-size: 44rpx;
}
.wuxbutton{
  width: 100%;
  position: fixed;
  bottom: 0;
} 
.wux-button--block{
  margin-top: 0rpx !important;
  margin-bottom: 0rpx !important;
}
.wux-button--full{
  margin-top: 0rpx !important;
  margin-bottom: 0rpx !important;
}
wux-button.wuxbutton button {
  border-radius: 0px;
}