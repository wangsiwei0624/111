@import '../../static/icon/iconfont.wxss';

page{
  height: 100%;
}
.page{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.hd{
  flex: 1;
  overflow: auto;
  background: #F7F9FA;
}
.mine{
  height: 307rpx;
  width: 100%;
  position: relative;
  /* border-bottom-left-radius: 40rpx;
  border-bottom-right-radius: 40rpx; */
  background: #F7F9FA;
}
.m-banner{
  height: 322rpx;
  width: 100%;
}
.mine-in{
  height: 307rpx;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.headimg{
  height: 130rpx;
  width: 130rpx;
  border-radius: 50%;
  background: #ffffff;
  position: absolute;
  top: 50rpx;
  left: 315rpx;
}
.headname{
  margin-top: 210rpx;
  color: #ffffff;
  text-align: center;
  font-size: 34rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
}
button.headname::after {
  border:0;
}

button.headname {
  background-color: transparent;
}

.list{
  margin-top: 58rpx;
}
.wux-cell-group__bd::after{
  border-top-width: 0rpx !important;
  border-bottom-width: 0rpx !important;
}
/* .wux-cell-group::after{
  border-top: 0rpx ;
  border-bottom: 0rpx !important;
} */
.list-cell{
  position: relative;
  
}
.list-cell button{
  height: 98rpx;
}

.list-cell .wux-cell__text{
  margin-left: 77rpx;
  font-size: 28rpx;
  font-weight:400;
  font-family:PingFangSC;
}
.list-cell:nth-last-child(1) .wux-cell__text{
  text-align: center;
  margin-left: 0rpx;  
}
/*  */
.list-cell .iconfont{
  color: #2A7AD7;
  position: absolute;
  top: 30rpx;
  left: 41rpx;
  font-size: 40rpx;
}
.wux-cell::after{
  border-bottom: 0rpx !important;
}

.page__bd{
  height: 100rpx;
}

.witem text{
  position: absolute;
  bottom: 4rpx;
  left: 334rpx;
}


