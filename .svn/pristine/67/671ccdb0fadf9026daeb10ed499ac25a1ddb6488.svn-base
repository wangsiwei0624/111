/* pages/report/index.wxss */
@import '../../static/icon/iconfont.wxss';
page{
  width: 100%;
  height: 100%;
}
.report-list-page {
  position: absolute;
  width: 100%;
  top: 0px;
  bottom: 0px;
} 
.wux-tabs{
  border-bottom: 1rpx solid #E4EBEE;
}
.list-contaner {
  position: absolute;
  top: 88rpx;
  width: 100%;
  height: calc(100% - 88rpx);
  bottom:0px;
  background-color: #F7F9FA;
}
.list-contaner .content-list {
  height: 100%;
}

.list-in {
  padding-top: 7.5rpx;
}

.list-no-data {
  text-align: center;
  padding-top: 40%;
}
.list-no-data image{
  width: 374rpx;
  height: 288rpx;
}
.content-list .list-item {
  margin: 25rpx 30rpx;
  background-color: #FFFFFF;
  border-radius:15rpx;
  padding: 20rpx;
}
.list-item .list-item-title {
  height:60rpx;
  line-height: 30rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.list-item .list-item-footer {
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
}
.list-item .list-item-footer .postion {
  display: inline-block;
  color: #A8BCD0;
  width: 370rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.list-item .list-item-footer .postion .iconfont{
  font-size: 20rpx;
  margin-right: 5rpx;
}
.list-item .list-item-footer .time {
  float: right;
  color: #A8BCD0
}

