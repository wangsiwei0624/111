@import '../../common/common.wxss';
page{
  background-color: #F7F9FA;
}
.page .head{
  background-color: #fff;
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.page .head .address .iconfont.addr{
  font-size:  30rpx;
  color: green;
}
.page .head .address .iconfont.iron{
  font-size:  25rpx;
  color: #A8BCD0;
}
.page .head .address text{
  color: #333;
  font-size: 26rpx;
}
.page .search .input{
  width: 420rpx;
  height: 60rpx;
  border: 1rpx solid #ddd;
  border-radius: 30rpx;
  text-indent: 20rpx;
  color: #333;
  font-size: 26rpx;
}
.page .search .placeholder{
  color: #ccc;
  font-size: 24rpx;
}
.page .search {
  position: relative;
}
.page .search .iconfont{
  position: absolute;
  top: 12rpx;
  right: 25rpx;
  color: #ddd;
  font-size: 35rpx;
}
.page .area{
  height: 80rpx;
  border-top: 1rpx solid #F7F9FA;
  background-color: #fff;
  padding-left: 29rpx;
}
.page .area .item{
  margin-right: 27rpx;
  display: inline-block;
}
.page .area .item text{
  font-size: 26rpx;
  color: #A8BCD0;
  line-height: 80rpx;
}
.page .area .item .line{
  width: 25rpx;
  height: 5rpx;
  background-color: #2A7AD7;
  margin: 0 auto;
  visibility: hidden;
}
.page .area .item.active text{
  color: #2A7AD7;
}
.page .area .item.active .line{
  visibility: visible;
}
.page .no-data{
  text-align: center;
}
.page .list{
  margin-top: 20rpx;
}
.page .list .item{
  height: 98rpx;
  background-color: #fff;
  line-height: 98rpx;
  color: #333;
  font-size: 28rpx;
  padding: 0 30rpx;
  margin-bottom: 10rpx;
  display: flex;
  justify-content: space-between;
}
.page .list .iconfont {
  color: #A8BCD0;
  font-size: 25rpx;
}