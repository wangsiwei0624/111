page{
  height: 100%;
}
.modal{
  height: 100%;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background:rgba(0,0,0,0.7);
  /* display: flex;
  flex-direction: column; */
  z-index: 1001;
}
.modal-in{
  position: relative;
} 
.mimg1{
  position: absolute;
  bottom: 18rpx;
  left: 23.5%;
  height: 300rpx;
  width: 408rpx;
}
.page{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  border-top: 1rpx solid #DEDEDE;
}
.list-no-data {
  text-align: center;
  padding-top: 10%;
}
.list-no-data image{
  width: 374rpx;
  height: 288rpx;
}
.hd{
  flex: 1;
  overflow: auto;
}

.hd .list{
  height: 220rpx;
  width: 100%;
  border-top: 1rpx solid #dddddd; 
  position: relative;
}
.hd .list image{
  height: 168rpx;
  width: 172rpx;
  border-radius: 5rpx;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}
.hd .list p:nth-child(1){
  position: absolute;
  top: 31rpx;
  left: 30rpx;
  width: 448rpx;
  height: 84rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  line-clamp:2;
  box-orient:vertical;
  white-space: normal;
  font-weight: bold;
  font-size: 30rpx;
}
.hd .list p:nth-child(2){
  position: absolute;
  bottom: 30rpx;
  left: 30rpx;
  font-size: 22rpx;
  color: #999999;
}
.page__bd{
  box-shadow:0px -2rpx 5rpx 0rpx rgba(0,0,0,0.1);
}
.list-on {
  text-align: center;
  padding-top: 40%;
}
@import '../../static/icon/iconfont.wxss';