@import '../../common/common.wxss';
page{
  background-color: #F7F9FA;
}
.page {
  margin-bottom: 30rpx;
}
.page .content{
  height: 80rpx;
  background-color: #fff;
  padding-left: 29rpx;
}
.page .content .item{
  margin-right: 27rpx;
  margin-bottom: 20rpx;
  display: inline-block;
}
.page .content .item text{
  font-size: 32rpx;
  color: #666;
  line-height: 80rpx;
}
.page .content .item.active text{
  font-weight: bold;
  color: #2A7AD7;
}

.page .point{
  height: 98rpx;
  background-color: #EAEFF1;
  padding-left: 20rpx;
}
.page .point .item{
  display: inline-block;
  margin-right: 20rpx;
}
.page .point .item text{
  color: #666;
  font-size: 26rpx;
  line-height: 98rpx;
}
.page .point .item.active text{
  color: #fff;
  background-color: #2A7AD7;
  border-radius: 30rpx;
  padding: 15rpx 20rpx; 
}



.page .sel-content{
  height:636rpx;
}
.page .sel-content .sel-body{
  border: 1rpx solid #EAEAEA;
  padding: 20rpx 30rpx;
}
.page .sel-content .title{
  display: inline-block;
  margin-bottom: 20rpx;
  height: 27rpx;
  border-bottom: 18rpx solid #9BC0EC;
}
.page .sel-content .title text{
  color: #333;
  font-size: 26rpx;
  line-height: 45rpx;
}
.page .sel-content .item{
  border: 1rpx solid #2A7AD7;
  padding: 18rpx 20rpx;
  color: #2A7AD7;
  font-size: 24rpx;
  border-radius: 30rpx;
  display: inline-block;
  margin-right: 30rpx;
  margin-bottom: 20rpx;
}
.page .sel-content .item.active{
  background-color: #2A7AD7;
  color: #fff;
}
/* 改变wux-pop的居中显示 */
.wux-popup__content{
  text-align: left !important;
}
.page .sel-content .sel-bottom{
  display: flex;
  justify-content: flex-end;
  padding-right: 79rpx; 
  line-height: 109rpx;
}
.page .sel-content .sel-bottom text{
  color: #333;
  font-size: 28rpx;
  margin-left: 70rpx;
}
.page .body {
  padding: 30rpx;
  margin-bottom: 98rpx;
}

.page .charts{
  position: relative;
  background-color: #fff;
  border-radius: 30rpx;
  height: 1037rpx;
}
.page .charts.height{
  height: 1600rpx;
}
.page .charts .date{
  width: 100%;
  height: 78rpx;
  display: flex;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  background-color: #E5EBEE;
}
.page .charts .date .item{
  color: #8297AE;
  font-size: 28rpx;
  text-align: center;
  line-height: 78rpx;
  width: 25%;
}
.page .charts .date .item:first-child{
  border-top-left-radius: 30rpx;
}
.page .charts .date .item:last-child{
  border-top-right-radius: 30rpx;
}
.page .charts .date .item.active{
  color: #2A7AD7;
  background-color: #fff;
}
.page .container{
  top: 10%;
  left: 0;
}
.page .container .no-data{
  text-align: center;
}
ec-canvas {
  width: 100%;
  height: 50%;
}
ec-canvas.none{
  display: none;
}
#mychart-dom-multi-bar {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

#mychart-dom-multi-scatter {
  position: absolute;
  top: 50%;
  bottom: 0;
  left: 0;
  right: 0;
}



.page .bottom{
  width: 100%;
  height: 98rpx;
  background-color: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  border-top: 1rpx solid #EAEFF1;
}
.page .place{
  width: 100%;
  height: 98rpx;
  background-color: #fff;
}
.page .bottom .item{
  width: 25%;
  line-height: 98rpx;
  color: #333;
  font-size: 28rpx;
  text-align: center;
  display: inline-block;
}
.page .bottom .item.active{
  color: #fff;
  background-color: #2A7AD7;
}