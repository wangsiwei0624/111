@import '../../common/common.wxss';
page{
  background-color: #F7F9FA;
}
wux-tabs{
  position: absolute;
  top: 0;
  width: 100%;
}
.list{
  background: #F7F9FA; 
  position: absolute;
  top: 88rpx;
  width: 100%;
  /* height: calc(100% - 88rpx); */
}
.list:nth-child(2){
  margin-top: 20rpx;
}
.page .tab-wrap{
  background-color: #fff;
  margin-top: 20rpx;
}
.page .tab-wrap .title{
  padding: 18rpx 0 18rpx 30rpx;
  border-bottom: 1rpx solid #eee;
}
.page .tab-wrap .title .icon{
  display: inline-block;
  width: 7rpx;
  height: 30rpx;
  background-color: #2A7AD7;
  vertical-align: middle;
}
.page .tab-wrap .title text{
  font-size: 30rpx;
  color:#2a7ad7;
  margin-left: 5rpx;
}
.page .tab-wrap .content{
  padding: 22rpx 30rpx;
}
.page .tab-wrap .content-nodata{
  padding: 22rpx 30rpx;
  color: #aaa;
  font-size: 28rpx;
}
.page .tab-wrap .content.last-content{
  margin-bottom: 120rpx;
}
.page .tab-wrap .content .info-row .label{
  color: #333;
  font-size: 28rpx;
}
.page .tab-wrap .content .info-row .value{
  color: #7D9AB6;
  font-size: 28rpx;
}
.page .tab-wrap .content .info-row .value.no-data{
  color: #aaa;
}
.page .tab-wrap .content .info-row{
  overflow: hidden;
}
.page .tab-wrap .content .info-row .left,
.page .tab-wrap .content .info-row .right{
  float: left; 
}
.page .tab-wrap .info-img image{
  width: 220rpx;
  height: 150rpx;
  margin-right: 10rpx;
}
/* 右边按钮 */
.page .right-btn .no-active{
  position: fixed;
  top: 50%;
  right:20rpx;
  display: flex;
  align-items: center;
}
.page .right-btn .active{
  position: fixed;
  top: 50%;
  right:20rpx;
  margin-top: -183rpx;
  display: flex;
  align-items: center;
}
.page .right-btn .btn-wrap{
  margin-right: 20rpx;
  z-index: 9999;
} 
.page .right-btn .btn-wrap .btn{
  width: 70rpx;
  height: 70rpx;
  border-radius: 70rpx;
  background-color: rgba(231, 169, 22, 0.9);
  border: 3rpx solid #fff;
  box-shadow:2rpx 2rpx 9rpx rgba(231, 169, 22, 0.5), -2rpx -2rpx 9rpx rgba(231, 169, 22, 0.5),2rpx -2rpx 9rpx rgba(231, 169, 22, 0.5),-2rpx 2rpx 9rpx rgba(231, 169, 22, 0.5); 
  text-align: center;
  color: #fff;
  line-height: 70rpx;
  font-size: 30rpx;
  margin-bottom: 28rpx;
}
.page .right-btn .btn-wrap .btn.blue{
  background-color: rgba(42,122,215,0.9);
  box-shadow:2rpx 2rpx 9rpx rgba(42,122,215, 0.5), -2rpx -2rpx 9rpx rgba(42,122,215, 0.5),2rpx -2rpx 9rpx rgba(42,122,215, 0.5),-2rpx 2rpx 9rpx rgba(42,122,215, 0.5); 
}
.page .right-btn .iron{
  width: 35rpx;
  height: 40rpx;
  background-color: rgba(226, 226, 226, 0.84);
  line-height: 30rpx;
  text-align: center;
  border-radius: 5rpx;
  
}
.page .right-btn .iron .iconfont{
  font-size: 20rpx;
  color: #333;
}

/* 历史警情 */
/* .page .list {
  margin-top: 20rpx;
} */
.page .list  .his-item{
  height: 98rpx;
  background-color: #fff;
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 10rpx;
}
.page .list  .his-item .title{
  font-size: 28rpx;
  color: #333;
  line-height: 98rpx;
  width: 350rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.page .list  .his-item .right .time{
  font-size: 24rpx;
  color: #999;
  line-height: 98rpx;
  margin-right: 20rpx;
} 
.page .list  .his-item .right .iconfont{
  font-size: 26rpx;
  color: #BFCEDD;
  line-height: 98rpx;
} 


/* 底部按钮 */
.page .bottom .bot-wrap{
  background-color: #fff;
  width: 100%;
  height: 100rpx;
  margin-top: 40rpx;
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  border-top: 1rpx solid #ddd;
}
.page .bottom .bot-wrap.big{
  justify-content: space-around;
  padding: 0;
}

.page .bottom .item{
  margin-right:28rpx;
  text-align: center;
}
.page .bottom .item.last{
  margin-right: 10rpx;
}
.page .bottom .item .iconfont{
  font-size: 51rpx;
  color: #BFCEDD;
}
/* .page .bottom .item .iconfont.finish{
  font-size: 70rpx;
} */
.page .bottom .item text{
  display: block;
  /* width: 40rpx; */
  color: #666;
  text-align: center;
  font-size: 20rpx;
}
.page .bottom .btn-item{
  margin-right:10rpx;
}
.page .bottom .btn-item .btn{
  width: 130rpx;
  height: 60rpx;
  border-radius: 65rpx;
  line-height: 60rpx;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
}
.page .bottom .btn-item .btn.big{
  width: 300rpx;
  height: 68rpx;
  border-radius: 68rpx;
  line-height: 69rpx;
  font-size: 28rpx;
}
.page .bottom .btn-item .green{
  background-color: #41CD62;
}
.page .bottom .btn-item .blue{
  background-color: #2A7AD7;
}
.page .bottom .item.disable text{
  color: #ddd;
}
.page .bottom .item.disable .iconfont{
  color: #ddd;
}
.page .bottom .btn-item.disable .green{
  background-color: #ddd;
  pointer-events: none;
}
.page .bottom .btn-item.disable .blue{
  background-color: #ddd;
  pointer-events: none;
}