@import '../../../static/icon/iconfont.wxss';
.page{
  width: 100%;
  height: 100%;
  background: #F7F9FA;
}
.list{
  width: 100%;
  
}
.list-in{
  width: 690rpx;
  margin: 35rpx auto;
}
.list-in-cell-title{
  width: 690rpx;
  height: 48rpx;
}
.title1{
  width: 80rpx;
  height: 30rpx;
  float: left;
  border-bottom: 18rpx solid #9BC0EC;
}
.title1 text{
  line-height: 45rpx;
  font-size:34rpx;
  font-family:PingFangSC;
  font-weight:600;
  color: #333333;
  margin-left: 6rpx;
  
}
.title2{
  width: 88rpx;
  height: 48rpx;
  background:linear-gradient(154deg,rgba(255,213,53,1) 0%,rgba(246,133,12,1) 100%);
  border-radius: 23rpx;
  float: right;
}
.title2 text{
  color: #ffffff;
  font-size: 22rpx;
  line-height: 48rpx;
  margin-left: 22rpx;
  font-family:PingFangSC;
  font-weight:400;
}
.list-in-cell-no{
  height: 100rpx;
  width: 690rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  background: #ffffff;
}
.list-in-cell-no text{
  display: flex;
  justify-content:center; 
  font-size: 28rpx;
  color: #BFCEDD;
  line-height: 100rpx;
  
}
.list-in-cell-detail{
  width: 690rpx;
  margin: 33rpx auto;
}
.list-in-cell-detail-cell{
  width: 690rpx;
  /* height: 134rpx; */
  background:#ffffff;
  border-radius:5px;
  margin: 10rpx auto;
}
.list-in-cell-detail-cell .detail-theme{
  font-size: 30rpx;
  line-height: 70rpx;
  /* margin-top: 21rpx; */
  margin-left: 20rpx;
  font-weight:400;
  color: #333333;
}
.list-in-cell-detail-cell .detail-img{
  /* margin-top: 14rpx; */
  margin-left: 20rpx; 
}
.list-in-cell-detail-cell .detail-img .iconfont{
  font-size: 30rpx;
  margin-right: 10rpx;
  color: #A8BCD0;
}
.list-in-cell-detail-cell .detail-address{
  font-size: 24rpx;
  color: #A8BCD0;
  /* margin-top: 18rpx; */
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  line-height: 66rpx;
}
.list-in-cell-detail-cell .detail-address .iconfont{
  font-size: 24rpx;
  margin-right: 5rpx;
}
.list-in-cell-detail-cell .detail-address .time{
  float: right;
  margin-right: 18rpx;
}
.list-in-cell-detail-cell .detail-timeslot{
  font-size: 24rpx;
  color: #A8BCD0;
  margin-left: 20rpx;
}
.list-in-cell-detail-cell .detail-timeslot .iconfont{
  font-size:  20rpx;
  margin-right: 5rpx;
}